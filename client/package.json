{
  "name": "property-praxis-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "VITE_PUBLIC_URL=\"\" vite --host",
    "build": "VITE_PUBLIC_URL=\"\" vite build",
    "lint": "eslint \"./src/**/*.{js,jsx,ts}\"",
    "lint:fix": "eslint --quiet --fix \"./src/**/*.{js,jsx,ts}\"",
    "prettier": "prettier --check \"./src/**/*.{js,jsx,css}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{js,jsx,css}\""
  },
  "dependencies": {
    "@sentry/react": "^7.93.0",
    "@turf/turf": "^6.5.0",
    "flat": "^5.0.2",
    "leaflet": "^1.7.1",
    "mapillary-js": "^4.1.0",
    "p-debounce": "^4.0.0",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-debounce-input": "^3.2.2",
    "react-dom": "^17.0.2",
    "react-map-gl": "5.2.1",
    "react-modal": "^3.14.4",
    "react-redux": "^7.2.6",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "^5.0.0",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "victory": "^36.3.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.1",
    "prettier-eslint": "^16.2.0",
    "sass": "^1.49.9",
    "vite": "^5.0.11"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "requireConfigFile": false,
      "ecmaFeatures": {
        "jsx": true
      },
      "babelOptions": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "import",
      "prettier",
      "react"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/prop-types": 0
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
